server {
  listen 80;
  server_name     .{{ domain_name }};

  access_log      /var/log/nginx/ipy.access.log;
  error_log       /var/log/nginx/ipy.error.log;

  location / {
    proxy_pass    http://localhost:8888;
  }

  location ~ ^/(js|css|img)/.*$ {
    root  {{ deploy_directory }}/repo/app/static;
  }
}

